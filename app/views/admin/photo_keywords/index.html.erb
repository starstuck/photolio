<h1>Listing photo keywords for <%= @photo.file_name -%></h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% for photo_keyword in @photo_keywords %>
  <tr>
    <td><%=h photo_keyword.name %></td>
    <td><%= link_to 'Show', admin_site_photo_keyword_path(@site, @photo, photo_keyword) %></td>
    <td><%= link_to 'Edit', edit_admin_site_photo_keyword_path(@site, @photo,
    photo_keyword) %></td>
    <td><%= link_to 'Destroy', admin_site_photo_keyword_path(@site, @photo, photo_keyword), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New photo keyword', new_admin_site_photo_keyword_path %> |
<%= link_to 'Back', admin_site_photos_path -%>
