<h2>New photo keyword for <%= @photo.file_name -%></h2>

<% form_for(@photo_keyword, :url => admin_site_photo_keywords_path) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', admin_site_photo_keywords_path %>
