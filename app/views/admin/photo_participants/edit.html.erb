<h1>Editing photo participant</h1>

<% form_for(@photo_participant, :url => admin_site_photo_participant_path) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :role %><br />
    <%= f.text_field :role %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', admin_site_photo_participant_path %> |
<%= link_to 'Back', admin_site_photo_participants_path %>
